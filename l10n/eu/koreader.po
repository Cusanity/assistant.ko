#: ../prompts.lua:20
msgid   "This prompt act as a dictionary for the highlighted text, to a word "
        "or phrase."
msgstr  "Gonbidapen honek hiztegi gisa jokatzen du nabarmendutako "
        "testuarekin, hitza edo esaldia."

#: ../prompts.lua:25
msgid   "Translate"
msgstr  "Itzuli"

#: ../prompts.lua:26
msgid   "This prompt translates the highlighted text to another language."
msgstr  "Gonbidapen honek nabarmendutako testua beste hizkuntza batera "
        "itzultzen du."

#: ../prompts.lua:48
msgid   "Simplify"
msgstr  "Sinplifikatu"

#: ../prompts.lua:50
msgid   "This prompt simplifies the highlighted text to make it easier to "
        "understand."
msgstr  "Gonbidapen honek nabarmendutako testua sinplifikatzen du, ulertzeko "
        "errazago izan dadin."

#: ../prompts.lua:60
msgid   "Explain"
msgstr  "Azaldu"

#: ../prompts.lua:62
msgid   "This prompt explains the highlighted text in detail, ensuring "
        "clarity and understanding."
msgstr  "Gonbidapen honek nabarmendutako testua xehetasunez azaltzen du, "
        "argitasuna eta ulermena bermatuz."

#: ../prompts.lua:73
msgid   "Summarize"
msgstr  "Laburbildu"

#: ../prompts.lua:75
msgid   "This prompt summarizes the highlighted text, capturing its main "
        "points and essential details."
msgstr  "Gonbidapen honek nabarmendutako testua laburbiltzen du, puntu "
        "nagusiak eta ezinbesteko xehetasunak jasoz."

#: ../prompts.lua:84
msgid   "Historical Context"
msgstr  "Testuinguru historikoa"

#: ../prompts.lua:86
msgid   "This prompt provides a detailed historical context for the "
        "highlighted text, explaining its significance and background."
msgstr  "Gonbidapen honek nabarmendutako testuaren testuinguru historiko "
        "zehatza eskaintzen du, bere garrantzia eta atze-oinarria azalduz."

#: ../prompts.lua:90
msgid   "Key Points"
msgstr  "Puntu garrantzitsuak"

#: ../prompts.lua:92
msgid   "This prompt extracts and lists the key points from the highlighted "
        "text, ensuring clarity and organization."
msgstr  "Gonbidapen honek nabarmendutako testutik puntu garrantzitsuak "
        "ateratzen eta zerrendatzen ditu, argitasuna eta antolaketa bermatuz."

#: ../prompts.lua:96
msgid   "ELI5"
msgstr  "ELI5"

#: ../prompts.lua:98
msgid   "This prompt explains the highlighted text as if to a five-year-old, "
        "simplifying complex concepts into easily understandable terms."
msgstr  "Gonbidapen honek nabarmendutako testua bost urteko bati azaltzen "
        "dio, kontzeptu konplexuak ulertzeko errazak diren terminoetan "
        "sinplifikatuz."

#: ../prompts.lua:102
msgid   "Grammar"
msgstr  "Gramatika"

#: ../prompts.lua:104
msgid   "This prompt analyzes the grammar of the highlighted text, providing "
        "a detailed explanation of its structure and any grammatical errors."
msgstr  "Gonbidapen honek nabarmendutako testuaren gramatika aztertzen du, "
        "bere egitura eta edozein gramatika-okerri buruzko azalpen zehatza "
        "emanez."

#: ../prompts.lua:109
msgid   "Vocabulary"
msgstr  "Hiztegia"

#: ../prompts.lua:111
msgid   "This prompt analyzes the vocabulary of the highlighted text, "
        "identifying complex words and providing definitions, synonyms, and "
        "usage examples."
msgstr  "Gonbidapen honek nabarmendutako testuaren hiztegia aztertzen du, "
        "hitz konplexuak identifikatuz eta definizioak, sinonimoak eta "
        "erabilera-adibideak emanez."

#: ../prompts.lua:127
msgid   "Wikipedia"
msgstr  "Wikipedia"

#: ../prompts.lua:129
msgid   "This prompt generates a comprehensive Wikipedia-style article based "
        "on the highlighted text, ensuring factual accuracy and neutrality."
msgstr  "Gonbidapen honek nabarmendutako testuaren oinarri Wikipedia estiloko "
        "artikulu zehatza sortzen du, egiazkotasuna eta neutraltasuna "
        "bermatuz."

